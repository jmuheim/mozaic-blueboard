      Call @PrepareGuitarPreset1
      Log {    üé∏ 1}
    
      Call @RecordNextLoopInGroup2
      Log {    ‚è∫Ô∏è 2}
    
    elseif step = 5
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    elseif step = 6
      Log {  Verse 4: You, you look like my brother‚Ä¶ (bass line)}
      Call @PrepareGuitarPreset2
      Log {    üé∏ 2}
    
      Call @RecordNextLoopInGroup2
      Log {    ‚è∫Ô∏è 2}
    
    elseif step = 7
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    elseif step = 8
      Log {  Verse 5: The village all gathered‚Ä¶ (arpeggio low)}
      Call @PrepareKeyboardPreset1
      Log {    üéπ 1}
    
      Call @RecordNextLoopInGroup2
      Log {    ‚è∫Ô∏è 2}
    
    elseif step = 9
      Call @ToggleSendKeyboard
      Log {    üéπ ‚úîÔ∏è}
    elseif step = 10
      Log {  Verse 5: The people of the village‚Ä¶ (arpeggio high)}
    elseif step = 11
    
    Log {Dramatic instrumentation}
    elseif step = 12
      Log {  1st guitar (low)}
      Call @PrepareGuitarPreset3
      Log {    üé∏ 3}
    
      Call @RecordNextLoopInGroup2
      Log {    ‚è∫Ô∏è 2}
    
    elseif step = 13
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    elseif step = 14
      Log {  2nd guitar (high)}
    elseif step = 15
    
      Call @Error
    endif

  //////////////////
  // Dolly Parton - Jolene
  //////////////////
  elseif preset = 4
    Log {Pre-Intro}
    if step = 1
      Log {  2x start of main riff}
      Call @ToggleSendMicrophone
      Log {    üé§ ‚ùå}
    
      Call @PrepareGuitarPreset1
      Log {    üé∏ 1}
      Call @ToggleSendGuitar
      Log {    üé∏ ‚úîÔ∏è}
    
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 7}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 6}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 5}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 4}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 3}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 2}
    
      Call @RecordNextLoopInGroup1
      Log {    ‚è∫Ô∏è 1}
    
    
    Log {Chorus}
    elseif step = 2
      Log {  Jolene‚Ä¶ (fingerstyle guitar)}
      Call @PrepareGuitarPreset1
      Log {    üé∏ 1}
    
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 3}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 4}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 5}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 6}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 7}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 8}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 9}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 10}
      Call @IncreaseClockLength
      Log {    ‚è≤Ô∏è 11}
    
      Call @RecordNextLoopInGroup2
      Log {    ‚è∫Ô∏è 2}
    
    elseif step = 3
      Call @ToggleSendMicrophone
      Log {    üé§ ‚úîÔ∏è}
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    
    Log {Verse 1}
    elseif step = 4
      Log {  Your beauty‚Ä¶ / Your smile‚Ä¶}
      Call @ToggleSendMicrophone
      Log {    üé§ ‚ùå}
    
      Call @PrepareGuitarPreset1
      Log {    üé∏ 1}
    
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 10}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 9}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 8}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 7}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 6}
      Call @DecreaseClockLength
      Log {    ‚è≤Ô∏è 5}
    
      Call @RecordNextLoopInGroup3
      Log {    ‚è∫Ô∏è 3}
    
    elseif step = 5
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    elseif step = 6
      Log {  He talks about‚Ä¶ / And I can easily understand‚Ä¶}
      Call @PrepareGuitarPreset2
      Log {    üé∏ 2}
    
      Call @RecordNextLoopInGroup3
      Log {    ‚è∫Ô∏è 3}
    
    elseif step = 7
      Call @ToggleSendGuitar
     Log {    üé∏ ‚úîÔ∏è}
    
    Log {Chorus}
    elseif step = 8
